{
  "name": "@twofortyfive/utils",
  "version": "0.0.21",
  "description": "Various utils",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "scripts": {
    "dev:clear": "rimraf node_modules .parcel-cache coverage dist",
    "dev:test": "jest --watch --coverage",
    "dev:format": "prettier --write \"src/**/*.ts\"",
    "dev:lint": "eslint src",
    "publish:test": "jest",
    "publish:build": "parcel build",
    "publish:ship": "npm run publish:test && npm run publish:build && npm publish --access public"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^9.5.0",
    "fast-check": "^3.19.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "parcel": "^2.12.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "prettier": {
    "printWidth": 120
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathieueveillard/@twofortyfive/utils.git"
  },
  "keywords": [],
